<#AlumnosMapping>
  rml:logicalSource [
    rml:source "https://raw.githubusercontent.com/liamosycka/cursoKG/main/Labs/morph-kg/data/alumnos.csv";
    rml:referenceFormulation ql:CSV ].

<#AlumnosMapping>
  rr:subjectMap [
    rr:template "http://example.com/Alumno/{ID}";
    rr:class :Alumno ].

<#AlumnosMapping>
  rr:predicateObjectMap [
    rr:predicate foaf:firstName;
    rr:objectMap [ rml:reference "Nombre"; rr:datatype xsd:string ] 
    ].

<#AlumnosMapping>
  rr:predicateObjectMap [
    rr:predicate foaf:surname;
    rr:objectMap [ rml:reference "Apellido"; rr:datatype xsd:string ] 
    ].

<#AlumnosMapping>
  rr:predicateObjectMap [
    rr:predicate :formacion;
    rr:objectMap [ rml:reference "Formaci√≥n"; rr:datatype xsd:string ] 
    ].

<#AlumnosMapping>
  rr:predicateObjectMap [
    rr:predicate foaf:mbox;
    rr:objectMap [ rml:reference "MAIL"; rr:datatype xsd:string ] 
    ].

<#AlumnosMapping>
  rr:predicateObjectMap [
    rr:predicate :cargo;
    rr:objectMap [ rml:reference "Cargo"; rr:datatype xsd:string ] 
    ].

<#AlumnosMapping>
  rr:predicateObjectMap [
    rr:predicate :inscripto;
    rr:objectMap [ rml:reference "Inscripto MCC"; rr:datatype xsd:boolean ] 
    ].

<#AlumnosMapping>
  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [ rml:reference "{Nombre, Apellido}"; rr:datatype xsd:string ] 
    ].
